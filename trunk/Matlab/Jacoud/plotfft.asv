function Uf=plotfft(u,fa,fmax)
%fa=1000; % freq amostragem
%t=[0:1/fa:2];
%u=sin(2*pi*10*t);
r=1; %20; % decimacao
f = fa/r; % freq. utilizada na FFT

Uf=fft(u);

w=[0:length(u)-1];
w=w*2*pi*f/length(u);


% vetor de frequencias em Hz
w/2/pi
%semilogx(w/2/pi,abs(Uf));
plot(Hz,abs(Uf));
xlabel('freq (Hz)');
%end